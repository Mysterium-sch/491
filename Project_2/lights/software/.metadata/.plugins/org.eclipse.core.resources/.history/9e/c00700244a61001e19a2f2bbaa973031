float pwm_frequency = 10e3f;
float dim_period = 2.0f;

int main()
{
	alt_u32 current_value;
	alt_u32 current_state;
	alt_u8 current_direction;
	alt_u32 keys;
	current_state=3;
	current_value=1;
	current_direction=0;
	printf ("Program running (UART)TRY...\n");
  	return 0;
}
